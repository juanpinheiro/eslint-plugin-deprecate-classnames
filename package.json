{
  "name": "eslint-plugin-deprecate-classnames",
  "version": "1.1.0-beta.0",
  "description": "An ESLint plugin with rules reporting usage of deprecated css classes",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --config .mocharc.cjs",
    "build": "babel lib --out-dir dist",
    "lint": "eslint lib tests",
    "fix": "eslint --fix lib tests",
    "format": "prettier --write .",
    "preversion": "npm run lint && npm run test",
    "prepublish": "npm run build"
  },
  "keywords": [
    "eslint",
    "deprecation",
    "deprecate function",
    "deprecate function eslint",
    "deprecate function eslint plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+git@github.com:juanpinheiro/eslint-plugin-deprecate-classnames.git"
  },
  "author": "Juan Pinheiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanpinheiro/eslint-plugin-deprecate-classnames/issues"
  },
  "homepage": "https://github.com/juanpinheiro/eslint-plugin-deprecate-classnames#readme",
  "peerDependencies": {
    "eslint": ">=7.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@evo/eslint-config-uaprom": "^6.0.0",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2"
  }
}
